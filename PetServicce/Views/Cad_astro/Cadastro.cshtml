<body style="font-family:Arial">

    <h1 style="text-align:center">PetService</h1>
    <h2 style="text-align:center">Formulario de cadastro</h2>

    <div style="width:400px;margin-left:50px">
        <form asp-action="Cadastro" name="form" class="form-group" style="background-color: #FAD371;padding: 20px;border-radius: 10px">
            <label asp-for="Nome" >Nome do dono</label>
            <input asp-for="Nome" class="form-control"  autocomplete="off"/>

            <label asp-for="Cpf" style="line-height:10px;margin-top:10px">CPF</label>
            <input asp-for="Cpf" class="form-control"  autocomplete="off"/>

            <label asp-for="NumeroContato" style="line-height:10px;margin-top:10px">Numero para contato</label>
            <input asp-for="NumeroContato" class="form-control"  autocomplete="off"/>

            <label asp-for="Endereco" style="line-height:10px;margin-top:10px">Endereço</label>
            <input asp-for="Endereco" class="form-control"  autocomplete="off"/>

       
            <label asp-for="Senha" style="line-height:10px;margin-top:10px">Informe uma senha para poder acessar seu perfil</label>
            <input asp-for="Senha" class="form-control"  autocomplete="off"/>

            <button type="button" class="btn btn-success" style="margin-top:10px" >Cadastrar</button>
            <button type="button" class="btn btn-danger" style="margin-top:10px">Recusar</button>
        </form>
    </div>

